{
  "name": "turbo-chatwoot-webhook",
  "version": "3.0.0",
  "description": "Webhook trung gian kết nối các platform với Chatwoot và Dify AI",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "lint": "eslint src/ scripts/ --ext .js",
    "lint:fix": "eslint src/ scripts/ --ext .js --fix",
    "build:frontend": "node scripts/build-frontend.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "yarn build:frontend && docker-compose up --build -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod:down": "docker-compose down",
    "docker:logs:dev": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:logs:prod": "docker-compose logs -f",
    "migrate": "node scripts/migrate.js",
    "migrate:status": "node scripts/migrate.js status",
    "seed": "node scripts/seed.js",
    "clean": "rm -rf nginx/html logs/*.log",
    "setup": "yarn migrate && yarn seed"
  },
  "packageManager": "yarn@1.22.19",
  "keywords": [
    "webhook",
    "chatwoot",
    "telegram",
    "dify",
    "chatbot"
  ],
  "author": "ThuanPT",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.0.0",
    "admin-lte": "3.2.0",
    "axios": "1.6.0",
    "bcrypt": "^6.0.0",
    "bootstrap": "4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "uuid": "^13.0.0",
    "vue3-toastify": "^0.2.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1"
  }
}
